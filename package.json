{
  "name": "monitor4",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "hmr": "ng serve --configuration=hmr --open",
    "hmr-col": "ng serve --configuration=hmrcol",
    "hmr-ven": "ng serve --configuration=hmrven",
    "build": "ng build --configuration production --build-optimizer",
    "test": "ng test",
    "test-cov": "ng test --code-coverage",
    "e2e": "ng e2e",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{ts,scss,less}\"",
    "tslint": "tslint \"src/**/*.ts\"",
    "stylelint": "stylelint \"src/**/*.{scss,less}\"",
    "post-build": "node ./build/post-build.js",
    "unused-css": "ngx-unused-css",
    "sonar": "sonar-scanner",
    "build-prod": "npm run build --configuration production --aot --build-optimizer",
    "build-qa-col": "ng build --configuration qa-col --output-hashing bundles --aot --build-optimizer --common-chunk",
    "build-qa-ven": "ng build --configuration qa-ven --output-hashing bundles --aot --build-optimizer --common-chunk",
    "build-prod-col": "ng build --configuration production-col --output-hashing bundles --aot --build-optimizer --common-chunk",
    "build-prod-ven": "ng build --configuration production-ven --output-hashing bundles --aot --build-optimizer --common-chunk",
    "bad-test": "ng build --configuration production-col --build-optimizer --source-map",
    "deploy-col": "cp -R app-col.yaml dist/app.yaml && cd dist && gcloud app deploy --project=armirene-369418 --no-promote --version",
    "deploy-ven": "cp -R app-ven.yaml dist/app.yaml && cd dist && gcloud app deploy --project=armirene-369418 --no-promote --version",
    "deploy-qa-col": "cp -R qa.yaml dist/app.yaml && cd dist && gcloud app deploy --project=armirene-369418 --no-promote --version",
    "deploy-qa-ven": "cp -R qa.yaml dist/app.yaml && cd dist && gcloud app deploy --project=armirene-369418 --no-promote --version",
    "bad-qa-col": "npm run build-qa-col && npm run deploy-qa-col",
    "bad-qa-ven": "npm run build-qa-ven && npm run deploy-qa-ven",
    "bad-col": "npm run build-prod-col && npm run deploy-col",
    "bad-ven": "npm run build-prod-ven && npm run deploy-ven",
    "bad-armi": "npm run build-prod-col && npm run deploy-armi",
    "host-col": "ng serve --configuration=hmrcol --disable-host-check --host 0.0.0.0",
    "host-ven": "ng serve --configuration=hmrven --disable-host-check --host 0.0.0.0"
  },
  "private": true,
